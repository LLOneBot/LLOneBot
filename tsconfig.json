{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./dist",
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "paths": {
      "@/common/*": [
        "./src/common/*"
      ],
      "@/onebot11/*": [
        "./src/onebot11/*"
      ],
      "@/ntqqapi/*": [
        "./src/ntqqapi/*"
      ],
      "@/milky/*": [
        "./src/milky/*"
      ]
    },
    "noEmit": true
  },
  "include": [
    "src",
    "scripts"
  ]
}
